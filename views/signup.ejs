<!DOCTYPE HTML>

<html>
	<head>
		<title>Binary by TEMPLATED</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<link rel="stylesheet" href="/public/assets/css/login.css" />
	</head>
	<body>
<div class="wrapper fadeInDown">
  <div id="formContent">
    <!-- Tabs Titles -->
    <h2 class="active">Register</h2>


    <!-- Icon -->
    <div class="fadeIn first">
      <img src="http://danielzawadzki.com/codepen/01/icon.svg" id="icon" alt="User Icon" />
    </div>

    <!-- Login Form -->
    <form>
      <input type="text"  class="fadeIn second" name="full_name" placeholder="Full Name">
      <input type="text"  class="fadeIn third" name="email" placeholder="Email">
        <input type="text" class="fadeIn fourth" name="age" placeholder="Age">
		<input type="button" class="fadeIn fiveth" value="Male">
		<input type="button" class="fadeIn sixth" value="Female">
      <input type="text"  class="fadeIn seventh" name="password" placeholder="Password">
	  <input type="text"  class="fadeIn eighth" name="password2" placeholder="Re-password">
        
      <input type="submit" class="fadeIn nineth" value="Register">
    </form>

    <!-- Remind Passowrd -->
    <div id="formFooter">
      <a class="underlineHover" href="#">Forgot Password?</a>
    </div>

  </div>
</div>
<script src="/public/assets/js/jquery.min.js"></script>
</body>
</html>